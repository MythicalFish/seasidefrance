{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@modules/*": ["src/modules/*"],
      "@pages/*": ["src/pages/*"],
      "@images/*": ["src/images/*"],
      "@photos/*": ["src/images/photos/*"],
      "@utils/*": ["src/utils/*"],
      "@data/*": ["src/data/*"],
      "@fixtures/*": ["src/data/_fixtures/*"],
      "@lib/*": ["src/lib/*"],
      "@styles/*": ["src/styles/*"],
      "@stores/*": ["src/stores/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
